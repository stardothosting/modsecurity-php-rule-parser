{
  "name": "stardothosting/secrule-parser",
  "description": "1:1 PHP port of msc_pyparser (ModSecurity SecRule parser) with JSON output",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "require": {
    "php": ">=7.4",
    "psr/log": "^1.1 || ^2.0 || ^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
