{
    "name": "stardothosting/modsecurity-php-rule-parser",
    "description": "A full ModSecurity rule parser implemented in PHP, supporting CoreRuleSet compatibility.",
    "type": "library",
    "require": {
        "php": "^8.0",
        "symfony/console": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "ModSecurity\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ModSecurity\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6"
    },
    "bin": [
        "bin/modsec-parser"
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true
}
