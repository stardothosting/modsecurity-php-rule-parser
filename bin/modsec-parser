#!/usr/bin/env php
<?php

require __DIR__ . '/../vendor/autoload.php';

use Symfony\Component\Console\Application;
use Stardothosting\ModSecurity\CLI\ParseRulesCommand;

$app = new Application('ModSecurity Parser CLI', '2.0.7');
$parseCommand = new ParseRulesCommand();
$parseCommand->setName('parse'); 
$app->add($parseCommand);
$app->run();
